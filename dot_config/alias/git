function gcp(){
    git commit -m $1
    git push
}

alias gs="git status"
alias ga="git add"
alias gaa="git add ."
alias gp="git push"

alias gf="git fetch"
alias gp="git pull"

alias gco="git checkout"
alias gcom="git checkout main"

alias gsoc="git diff ORIG_HEAD HEAD"

alias gitroot='cd $(git rev-parse --show-toplevel)'
